{
  "description": "A Neos Flow package to easily use the mailgun api",
  "type": "neos-package",
  "name": "neosrulez/neos-mailgun",
  "require": {
    "neos/flow": "*",
    "symfony/http-client": "v5.4.3",
    "nyholm/psr7": "^1.0",
    "mailgun/mailgun-php": "v3.5.1"
  },
  "autoload": {
    "psr-4": {
      "NeosRulez\\Neos\\Mailgun\\": "Classes/"
    }
  },
  "extra": {
    "neos": {
      "package-key": "NeosRulez.Neos.Mailgun"
    }
  }
}